@page "/job_locations/create/"
@inject HttpClient _client
@inject NavigationManager _navManager

<h3 class="card-title">Create New Regions</h3>
<hr />
<br />

<FormComponent ButtonText="Create Job_location" job_locations="job_locations" OnValidSubmit="@CreateJob_location" />

@code {
    job_location job_locations = new job_location();

    private async Task CreateJob_location()
    {
        await _client.PostAsJsonAsync(Endpoints.Job_locationsEndpoint, job_locations);
        _navManager.NavigateTo("/job_locations/");
    }
}
